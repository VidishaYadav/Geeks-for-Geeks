class GfG
{
   /* Prints preorder traversal of Binary Tree.  In output
      all keys should be separated by space. For example
      preorder traversal of below tree should be "10 20 30"
         10
       /      \ 
    20       30 */
    void preorder(Node root)
    {
       // Your code goes here
       if(root == null)
       return;
       
       System.out.print(root.data+" ");
       preorder(root.left);
       preorder(root.right);
    }
}
